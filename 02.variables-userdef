- name: Play-level variables
  hosts: localhost
  vars:
    URL: avinash@google.com
  tasks:
    - name: Print URL
      ansible.builtin.debug:
        msg: "{{URL}}"

- name: Variables from a file
  hosts: localhost
  vars_files:
    - demo-vars.yml
  tasks:
    - name: Print URL
      ansible.builtin.debug:
        msg: "{{URL}}"



